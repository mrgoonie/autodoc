# AutoDocAI Environment Configuration Example
# Copy this file to .env and fill in your values

# API Keys (required)
OPENROUTER_API_KEY=your_openrouter_api_key_here
GITHUB_PAT=your_github_personal_access_token_here

# Optional API Keys
SENDGRID_API_KEY=your_sendgrid_api_key_here
QDRANT_API_KEY=your_qdrant_api_key_here

# Note: GITHUB_REPO_URL and GITHUB_BRANCH are now handled via CLI arguments
# GITHUB_REPO_URL is required for every call
# GITHUB_BRANCH is optional (default is 'main')

# OpenRouter Configuration
OPENROUTER_MODEL=anthropic/claude-3-sonnet
OPENROUTER_API_BASE=https://openrouter.ai/api/v1

# SendGrid Configuration
SENDGRID_FROM_EMAIL=noreply@autodocai.com
SENDGRID_SUCCESS_TEMPLATE=your_sendgrid_success_template_id
SENDGRID_FAILURE_TEMPLATE=your_sendgrid_failure_template_id

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION=autodoc_embeddings
QDRANT_IN_MEMORY=True

# AutoDocAI Configuration
AUTODOC_OUTPUT_DIR=./output
AUTODOC_TEMP_DIR=./temp
AUTODOC_SUPPORTED_LANGUAGES=python,javascript,java
AUTODOC_DEFAULT_LANGUAGE=en
AUTODOC_TRANSLATION_LANGUAGES=en,vi
